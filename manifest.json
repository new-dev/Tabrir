{
  "manifest_version": 2,

  "name": "Tabrir",
  "description": "Tabrir allows you to keep collections of websites that will all open on a single click",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "resources/tabrirIcon16.png",
    "default_popup": "views/popup.html"
  },
  "icons": {
    "16": "resources/tabrirIcon16.png",
    "48": "resources/tabrirIcon48.png",
    "128": "resources/tabrirIcon128.png",
    "300": "resources/tabrirIcon300.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
